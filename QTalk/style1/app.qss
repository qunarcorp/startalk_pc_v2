}

QFrame#search:focus {
    background-color: rgba(255,255,255,1);
    border:1px solid rgba(0,202,190,1);
}

QLineEdit#searchEdt{
	background-color: rgba(0,0,0,0);
	border-width:0;
	border-style:outset;
	font-weight:400;
	font-size: 13px;
}

QToolButton#searchBtn {
    background-color: rgba(0,0,0,0);
	border-image:url(:/QTalk/image1/search_normal.png);
}
QToolButton#searchBtn:hover{
	border-image:url(:/QTalk/image1/search_hover.png);
}

QFrame#MessageBox {
    background:rgba(255,255,255,1);
    border-radius:4px;
    border:1px solid rgba(181,181,181,1);
}

QPushButton#NoButton {
    background:rgba(255,255,255,1);
