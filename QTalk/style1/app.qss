}

QPushButton#YesButton {
    background:rgba(0,202,190,1);
    border-radius:2px;
    font-size:14px;
    font-weight:500;
    color:rgba(255,255,255,1);
    line-height:20px;
}

QPushButton#DeleteButton {
    background:rgba(235,84,95,1);
    border-radius:2px;
    border:1px solid rgba(217,217,217,1);
    font-size:14px;
    font-weight:500;
    color:rgba(255,255,255,1);
