QMenu{
	background: rgba(255, 255, 255, 1);
    border-radius: 6px;
    border-width: 1px;
    border-style: groove;
    border-color:rgba(181, 181, 181, 1);
    padding:5px 0px;
}

QMenu::item{
    min-width: 100px;
	min-height: 32px;
    color:rgba(51, 51, 51, 1);
    font-size:14px;
	padding: 2px 10px;
	border-radius: 4px;
	margin: 2px 0px;
	background: rgba(255, 255, 255, 1);
}

QMenu::separator
{
    height: 1px;
    background: rgba(220,220,220,1);
}

QMenu::item:hover{
	background-color: rgba(242, 242, 242, 1);
}

QMenu::item:selected{
	background-color: rgba(242, 242, 242, 1);
}

QMenu::item:disabled{
    color: #B0B0B0;
}

QSplitter::handle{
	background:rgba(238,238,238,1);
	width: 1px;
}

/**
 * @brief 滚动条设置水平
 * @author cyz
 * @date 2018.9.19
 */
QScrollBar:horizontal
{
    height: 17px;
    margin: 5px 0px 5px 0px; /*上 右 下 左*/
	background-color: rgba(255, 255, 255, 0);
    /*border: 1px transparent rgba(255,255,255,100%);
    border-radius: 5px;*/
}

QScrollBar::handle:horizontal
{
    background-color:rgba(0,0,0,0.2002);
    min-width: 30px;
    border-radius: 3px;
}
QScrollBar::handle:horizontal:hover
{
	background-color:rgba(0,0,0,0.4);
    min-height: 30px;
    border-radius: 3px;
}

QScrollBar::add-line:horizontal
{
    margin: 0px 0px 0px 0px;
    border-image: url(./style/style1/image/com/add-line_horizontal.png);
    height: 14px;
    width: 7px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
    margin: 0px 0px 0px 0px;
    border-image: url(./style/style1/image/com/sub-line_horizontal.png);
    height: 14px;
    width: 7px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:hover,QScrollBar::add-line:horizontal:on
{
    border-image: url(./style/style1/image/com/add-line_horizontal_hover.png);
    height: 14px;
    width: 7px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}


QScrollBar::sub-line:horizontal:hover, QScrollBar::sub-line:horizontal:on
{
    border-image: url(./style/style1/image/com/sub-line_horizontal_hover.png);
    height: 14px;
    width: 7px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
    background: none;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
    background: none;
}

/**
 * @brief 滚动条设置垂直
 * @author cyz
 * @date 2018.9.19
 */
QScrollBar:vertical
{
    width: 17px;
    margin: 0px 5px 0px 5px; 
	background-color: rgba(255,255,255,0);
    /*border: 1px transparent rgba(255,255,255,100%);
    border-radius: 5px;*/
}


QScrollBar::handle:vertical
{
	background-color:rgba(0,0,0,0.2002);
    min-height: 30px;
    border-radius: 3px;
}

QScrollBar::handle:vertical:hover
{
	background-color:rgba(0,0,0,0.4);
    min-height: 30px;
    border-radius: 3px;
}


QScrollBar::sub-line:vertical
{
    margin: 0px 0px 0px 0px;
  /*  border-image: url(./style/style1/image/com/sub-line_vertical.png); */
    height: 0px;
    width: 0px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical
{
    margin: 0px 0px 0px 0px;
  /*  border-image: url(./style/style1/image/com/add-line_vertical.png);*/
    height: 0px;
    width: 0px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:hover,QScrollBar::sub-line:vertical:on
{

    /*border-image: url(./style/style1/image/com/sub-line_vertical_hover.png);*/
    height: 0px;
    width: 0x;
    subcontrol-position: top;
    subcontrol-origin: margin;
}


QScrollBar::add-line:vertical:hover, QScrollBar::add-line:vertical:on
{
   /* border-image: url(./style/style1/image/com/add-line_vertical_hover.png);*/
    height: 0px;
    width: 0px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}


QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical
{
    background: none;
}


QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical
{
    background: none;
}

QFrame#MainWindowMainFrm{
    background-color: rgba(255,255,255,1);
    border-top-left-radius:8px;
    border-top-right-radius:8px;
    border-bottom-right-radius:0px;
    border-bottom-left-radius:0px;
}

QFrame#MainWindowMainBottomFrm{
    background-color: rgba(255,255,255,1);
    border-top-left-radius:0px;
    border-top-right-radius:0px;
    border-bottom-right-radius:6px;
    border-bottom-left-radius:6px;
}

QToolTip {
    min-width: 50px;
    min-height: 24px;
    font-size:12px;
    color:rgba(255,255,255,1);
    background: rgba(0, 0, 0, 0.75);
    line-height:12px;
    border: 2px solid rgba(0, 0, 0, 0.75);
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
}

QPushButton#ModButton {
    border-image: url(:/QTalk/image1/edit_normal.png);
}

QPushButton#ModButton:hover {
    border-image: url(:/QTalk/image1/edit_hover.png);
}

/**SearchEdit*/
QFrame#search
{
    background-color: rgba(255,255,255,1);
    border:1px solid rgba(151, 151, 151, 0.151);
