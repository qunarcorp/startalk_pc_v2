QComboBox {
    height:30px;
    background:rgba(255,255,255,1);
    border-radius:6px;
    border:1px solid rgba(181,181,181,1);

    font-size:14px;
    font-weight:400;
    color:rgba(51,51,51,1);
    line-height:14px;

    padding-left :20px;
}

QComboBox::drop-down{
    background-color: rgba(255,255,255,1);
    subcontrol-position: top right;
    subcontrol-origin: padding;
    image:url(:/QTalk/image1/arrow_sort_down_small.png);
    margin-top: 5px;
}

QComboBox::item {
    background-color: rgba(255,255,255,1);
    height:30px;
    border-radius:2px;
}

QComboBox::item:hover, QComboBox::item:selected {
    background-color: rgba(242,242,242, 1);
}

QFrame#Line {
    background:rgba(238,238,238,1);
}

/** QRadioButton **/
QRadioButton{
    spacing: 5px;
    color:#333333;
    font-size:13px;
    font-weight:400;
}

QRadioButton::indicator{
    width: 20px;
    height: 20px;
}

QRadioButton::indicator:unchecked{
    image:url(:/QTalk/image1/radio_icon_normal.png);
}

QRadioButton::indicator:checked{
    image:url(:/QTalk/image1/radio_icon_selected.png);
}

QRadioButton::indicator:checked:disabled{
    image:url(:/QTalk/image1/radio_icon_selected.png);
}

/** QCheckBox **/
QCheckBox{
    spacing: 5px;
    color:#333333;
    font-size:13px;
    font-weight:400;
}

QCheckBox::indicator{
    width: 20px;
    height: 20px;
}

QCheckBox::indicator:unchecked{
    image:url(:/QTalk/image1/checkbox_icon_uncheck.png);
}

QCheckBox::indicator:checked{
    image:url(:/QTalk/image1/checkbox_icon_check.png);
}

QCheckBox::indicator:checked:disabled{
    image:url(:/QTalk/image1/checkbox_icon_check.png);
}

